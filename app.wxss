/**app.wxss**/
page{
  background: #fff;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
.container {
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box; 
} 
.cz {color:#ffe456;}
.cf1{color:#fc6676;}
.cf2{color:#6490fe;}
.cf3{color:#70e48c;}
.bg_cz {background-color:#ffe456;color: #fff;}
.bg_cf1{background-color:#fc6676;color: #fff;}
.bg_cf2{background-color:#6490fe;color: #fff;}
.bg_cf3{background-color:#70e48c;color: #fff;}
.ch1{color: #302e40;}
.ch2{color: #484858;}
.ch3{color: #7a7a84;}
.ch4{color: #9c9ca4;}
.ch5{color: #dedee0;}
.ch6{color: #f2f4f0;}
.ch7{color: #fff;}
.bd_ch1{border:1px solid #302e40;}
.bd_ch2{border:1px solid #484858;}
.bd_ch3{border:1px solid #7a7a84;}
.bd_ch4{border:1px solid #9c9ca4;}
.bd_ch5{border:1px solid #dedee0;}
.bd_ch6{border:1px solid #f2f4f0;}
.t1{font-size: 34rpx;}
.t2{font-size: 32rpx;}
.t3{font-size: 30rpx;}
.t4{font-size: 28rpx;}
.t5{font-size: 24rpx;}
.t6{font-size: 20rpx;}
/*隐藏滚动条*/
::-webkit-scrollbar {
    width: 0px;
    height: 1px;
}
.head-back{
  position: absolute;
  top:0;
  left: 0;
  height: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.24);
  z-index: 1;
}
.head-face{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 2;
}
view,scroll-view{
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
p{
  display: block;
}
button::after{
  border: none;
}
.loading{
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 100;
  background: #fff;
}
